<?xml version="1.0" encoding="UTF-8"?>
<restful-faces>
	<services>
		<service url="/test/{param2}/{param1}" method="GET"
			mime="application/json">
			<handler bean="bean" method="method">
				<parameter index="0" name="param1" />
				<parameter index="1" name="param2" />
			</handler>
		</service>
	</services>
	<genericDataFormatters>
		<dataFormatter class="com.bradchen.faces.rest.data.JsonDataAdapter" />
	</genericDataFormatters>
	<genericDataParsers>
		<dataParser class="com.bradchen.faces.rest.data.JsonDataAdapter" />
	</genericDataParsers>
</restful-faces>
